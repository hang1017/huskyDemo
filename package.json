    "*.css": "stylelint --fix"
  },
  "dependencies": {
    "umi": "^4.0.13"
  },
  "devDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@umijs/fabric": "^3.0.0",
    "@umijs/lint": "^4.0.42",
    "chalk": "^5.2.0",
    "eslint": "^8.31.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "fs": "^0.0.1-security",
    "lint-staged": ">=10",
    "stylelint": "^14.16.1",
    "typescript": "^4"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js",
    "pre-commit": "lint-staged"
  }
}
